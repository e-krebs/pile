{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": false,
        "webextensions": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "no-var": "warn",
        "valid-typeof": "warn"
    },
    "globals": {
        "Vibrant": true,
        "app": true,
        "angular": true,
        "date": true
    }
}